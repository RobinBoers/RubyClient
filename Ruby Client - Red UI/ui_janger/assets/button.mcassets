    }
  },

  "classic_legacy_action_button@button_java.classic_legacy_button_150px": {
    "$pressed_button_name|default": "unset_button_name",
    "$button_label|default": "unset $button_label"
  },

  //--------------------------------------------------------------------------------
  // Button Java: Button State
  //--------------------------------------------------------------------------------

  "button_state": {
    "type": "panel",
    "$button_alpha|default": 1,
    "$button_label|default": "",
    "$button_label_color|default": [],
    "$uv_size_image_right|default": "$uv_size_image",
    "$uv_size_image_left|default": "$uv_size_image",
    "$button_content|default": "",
    "controls": [
      { "$button_content@$button_content": {} },
      {
        "image_content": {
          "type": "stack_panel",
          "orientation": "horizontal",
          "size": [ "100%", "100%" ],
          "visible": "$show_button_image",
          "$show_button_image|default": true,
          "controls": [
            {
              "left_image": {
                "type": "image",
                "texture": "$widgets_texture",
                "uv": "$left_uv_image",
                "uv_size": "$uv_size_image_left",
                "size": "$left_division_percentage",
                "alpha": "$button_alpha",
                "layer": 2
              }
            },
            {
              "right_image": {
                "type": "image",
                "texture": "$widgets_texture",
                "size": "$right_division_percentage",
                "uv": "$right_uv_image",
                "uv_size": "$uv_size_image_right",
                "alpha": "$button_alpha",
                "layer": 2
              }
            }
          ]
        }
      },
      {
        "label_content": {
          "type": "label",
          "text": "$button_label",
          "color": "$button_label_color",
          "shadow": true,
          "alpha": "$button_alpha",
          "anchor_from": "center",
          "anchor_to": "center",
          "layer": 10,
          "bindings": "$button_label_bindings",
          "$button_label_bindings|default": []
        }
      }
    ]
  }
}