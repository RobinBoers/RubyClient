               "controls": [
                 {
                   "available_pack_description_label@resource_packs.available_pack_description_label": {
                     "layer": 1,
                     "size": [ "100%-2px", "100%" ],
                     "offset": [ 1, 0 ]
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  },

