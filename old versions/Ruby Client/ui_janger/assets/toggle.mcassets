  "button_toggle_state": {
    "type": "panel",
    "$toggle_label|default": "",
    "font_type": "smooth",
    "$toggle_label_color|default": [],
    "$uv_size_image_left|default": "$uv_size_image",
    "$uv_size_image_right|default": "$uv_size_image",
    "controls": [
      {
        "image_content": {
          "type": "stack_panel",
          "orientation": "horizontal",
          "size": [ "100%", "100%" ],
          "visible": "$show_button_toggle_image",
          "$show_button_toggle_image|default": true,
          "controls": [
            {
              "left_image": {
                "type": "image",
                "texture": "$widgets_texture",
                "uv": "$left_uv_image",
                "uv_size": "$uv_size_image_left",
                "size": "$left_division_percentage",
                "layer": 2
              }
            },
            {
              "right_image": {
                "type": "image",
                "texture": "$widgets_texture",
                "size": "$right_division_percentage",
                "uv": "$right_uv_image",
                "uv_size": "$uv_size_image_right",
                "layer": 2
              }
            }
          ]
        }
      },
      {
        "label_content": {
          "type": "label",
          "text": "$button_toggle_label",
          "font_type": "smooth",
          "color": "$button_toggle_label_color",
          "shadow": true,
          "anchor_from": "center",
          "anchor_to": "center",
          "layer": 10
        }
      }
    ]
  },

  "option_toggle_label": {
    "type": "label",
    "text": "$option_toggle_label",
    "font_type": "smooth",
    "color": "$option_toggle_label_color",
    "shadow": true,
    "font_size": "$toggle_font_size",
    "text_alignment": "right",
    "anchor_from": "center",
    "anchor_to": "center",
    "layer": 10
  },

  "option_toggle_state": {
    "type": "panel",
    "$option_toggle_label|default": "",
    "$option_toggle_label_color|default": [],
    "font_type": "smooth",
    "$toggle_font_type": "smooth",
    "$toggle_font_size": "normal",
    "$argument|default": ": ",
    "$spacer|default": true,
    "$state_on|default": false,
    "$state_off|default": false,
    "$space": " ",
    "$state_off_text|default": "options.off",
    "$state_on_text|default": "options.on",
    "controls": [
      {
        "image_content": {
          "type": "stack_panel",
          "orientation": "horizontal",
          "size": [ "100%", "100%" ],
          "controls": [
            {
              "left_image": {
                "type": "image",
                "texture": "$widgets_texture",
                "uv": "$left_uv_image",
                "uv_size": "$uv_size_image",
                "size": "$left_division_percentage",
                "layer": 2
              }
            },
            {
              "right_image": {
                "type": "image",
                "texture": "$widgets_texture",
                "size": "$right_division_percentage",
                "uv": "$right_uv_image",
                "uv_size": "$uv_size_image",
                "layer": 2
              }
            }
          ]
        }
      },
      {
        "toggle_labels": {
          "type": "stack_panel",
          "orientation": "horizontal",
          "size": [ "default", 10 ],
          "controls": [
            { "label@option_toggle_label": { } },
            {
              "text_spacer@option_toggle_label": {
                "ignored": "$spacer",
                "text": "$space"
              }
            },
            {
              "spacer_argument@option_toggle_label": {
                "visible": "$spacer",
                "text": "$argument"
              }
            },
            {
              "state_label_on@option_toggle_label": {
                "visible": "$state_on",
                "ignored": "$state_off",
                "text_alignment": "left",
                "font_type": "smooth",
                "text": "$state_on_text"
              }
            },
            {
              "state_label_off@option_toggle_label": {
                "visible": "$state_off",
                "ignored": "$state_on",
                "text_alignment": "left",
                "font_type": "smooth",
                "text": "$state_off_text"
              }
            }
          ]
        }
      }
    ]
  },

  "custom_toggle_state": {
    "type": "panel",
    "controls": [
      {
        "image_content": {
          "type": "image",
          "texture": "$texture_to_toggle",
          "size": [ "100%", "100%" ],
          "visible": "$show_button_toggle_image",
          "$show_button_toggle_image|default": true,
          "uv": "$uv_image",
          "uv_size": "$uv_size_image",
          "layer": 2
        }
      },
      {
        "custom_toggle_content@$custom_toggle_content": {
          "anchor_from": "center",
          "anchor_to": "center",
          "layer": 10
        }
      }
    ]
  }
